{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": 0,
        "no-console": 0,
        "no-alert": 0,
        "import/no-cycle": 0,
        "import/no-mutable-exports": 0
    }
}
